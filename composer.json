    "description": "Simple PHP client for Akamai Analytics {Open} API",
    "version": "0.2.0",
    "keywords": ["analytics", "akamai", "open", "api", "media", "delivery"],
    "homepage": "http://www.mobilerider.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Perez",
            "email": "michel@mobilerider.com"
        }
    ],
    "autoload": {
        "psr-4": {
